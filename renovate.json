{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["clusters/home-cluster/.+helm-?release\\.yaml$"],
      "matchStrings": [
        "registryUrl=(?<registryUrl>.*?)\n *chart: (?<depName>.*?)\n *version: (?<currentValue>.*)\n"
      ],
      "datasourceTemplate": "helm"
    }
  ]
}
